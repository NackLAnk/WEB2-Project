{
    "version": 2,
    "builds": [
      { "src": "package.json", "use": "vercel-sapper" }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server.js",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
      },
      {
        "src": "/(.*)",
        "dest": "client/public/$1"
      }
    ]
  }
  